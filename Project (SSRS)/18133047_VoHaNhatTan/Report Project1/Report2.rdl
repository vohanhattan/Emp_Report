        <QueryParameters>
          <QueryParameter Name="@TK_NamNu">
            <Value>=Parameters!TK_NamNu.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DimDepartment.dept_name, FactSex.Female, FactSex.Male
FROM     DimDepartment INNER JOIN
                  FactSex ON DimDepartment.dept_no = FactSex.dept_no
WHERE  (DimDepartment.dept_name IN (@TK_NamNu))</CommandText>
      </Query>
