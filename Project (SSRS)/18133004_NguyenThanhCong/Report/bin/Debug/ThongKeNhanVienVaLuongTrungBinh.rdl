  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Employees</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Department">
            <Value>=Parameters!Department.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT FactHeadcount.Total_Employees, FactHeadcount.Avg_Salaries, DimDepartment.dept_name
FROM     FactHeadcount INNER JOIN
                  DimDepartment ON FactHeadcount.departmentKey = DimDepartment.departmentKey
where DimDepartment.dept_name in (@Department)</CommandText>
      </Query>
      <Fields>
        <Field Name="dept_name">
          <DataField>dept_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Employees">
          <DataField>Total_Employees</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Avg_Salaries">
          <DataField>Avg_Salaries</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>Employees</DataSourceName>
        <CommandText>select distinct dept_name as pb
from DimDepartment
order by pb</CommandText>
      </Query>
      <Fields>
        <Field Name="pb">
          <DataField>pb</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
