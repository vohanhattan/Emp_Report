  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Employees</DataSourceName>
        <CommandText>SELECT DimDepartment.dept_name, FactHeadcount.Total_Employees, FactHeadcount.Avg_Salaries
FROM     DimDepartment INNER JOIN
                  FactHeadcount ON DimDepartment.departmentKey = FactHeadcount.departmentKey</CommandText>
      </Query>
      <Fields>
        <Field Name="dept_name">
          <DataField>dept_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Employees">
          <DataField>Total_Employees</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Avg_Salaries">
          <DataField>Avg_Salaries</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
